rnd=$RANDOM
grp=az204-cosmosdb-$rnd
acct=cosmosacctap
dbname=productsdb

az cosmosdb create --name $acct --kind GlobalDocumentDB -g $grp

az cosmosdb sql database create --account-name $acct --name $dbname -g $grp

az cosmosdb sql container create --account-name $acct --database-name $dbname --name products --partition-key-path "/city"  --throughput 1000 -g $grp
    